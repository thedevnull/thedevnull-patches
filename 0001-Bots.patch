+                        const ItemPrototype *const pItemProto = pItem->GetProto();
+                        std::string name = pItemProto->Name1;
+
+                        out << " |cffffffff|Hitem:" << pItemProto->ItemId << ":0:0:0:0:0:0:0" << "|h[" << name << "]|h|r";
+                        if(pItem->GetCount() > 1)
+                            out << "x" << pItem->GetCount() << ' ';
