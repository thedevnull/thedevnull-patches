+
+
+#define SPELL_MARK_OF_THE_WILD      SPELL_MARK_OF_THE_WILD_A[SPELL_LEVEL]
+#define SPELL_THORNS                SPELL_THORNS_A[SPELL_LEVEL]
+#define SPELL_HEALING_TOUCH         SPELL_HEALING_TOUCH_A[SPELL_LEVEL]
