+			intimidatingShout_cd = 0;
+			pummel_cd = 0;
+			whirlwind_cd = 0;
+			bloodrage_cd = 0;
+			disarm_cd = 0;
+			intervene_cd = 0;
+			shieldBash_cd = 0;
+			spellReflection_cd = 0;
+			potion_cd = 0;
+			firstAid_cd = 0;
+			pvpTrinket_cd = 0;
+			taunt_cd = 0;
+			sunder_cd = 0;
+			rage = 0;
+			yellRage = 0;
+			GCD = 0;
+			castDemoralizingShout = false;
+			battleStance = true;
+			defensiveStance = false;
