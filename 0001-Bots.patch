+		{
+			charge_cd = 0;
+			deathWish_cd = 0;
+			mortalStrike_cd = 0;
+			overpower_cd = 0;
+			retaliation_recklessness_shieldwall_cd = 0;
+			berserkerRage_cd = 0;
+			challengingShout_cd = 0;
+			battleShout_cd = 0;
